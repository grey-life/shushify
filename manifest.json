{
    "name": "Shushify",
    "version": "1.0",
    "description": "Mute Sportify ads",
    "permissions": ["storage","declarativeContent","activeTab","tabs"],
    "background": {
        "scripts": ["background.js"],
        "presistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/shush16.png",
        "32": "images/shush32.png",
        "48": "images/shush48.png",
        "128": "images/shush128.png"
    },
    "manifest_version": 2
}
